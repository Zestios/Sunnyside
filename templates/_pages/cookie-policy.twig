{# @var craft \craft\web\twig\variables\CraftVariable #}
{% set pageClass = 'app-newsfeed' %}
{% extends "_layouts/_layout.twig" %}

{% block _inline_css %}
  <style>
    {{ source ("_pages/cookie-policy_critical.min.css", ignore_missing = true) }}
  </style>
{% endblock %}

{% block aside %}{% endblock %}

{% block content %}

  <section class="terms-wrapper">
    <div class="terms-content">
      {{ entry.cookiePolicy|raw }}
    </div>
  </section>

{% endblock %}

{% block _inline_js %}
{% endblock %}
